{
  "name": "testcafe-example",
  "version": "1.0.0",
  "description": "Cloud web application functional end-to-end test project",
  "repository": "https://github.com/hortonworks/cloudbreak-e2e-protractor.git",
  "author": "Hortonworks Inc. (https://hortonworks.com)",
  "license": "MIT",
  "main": "tests/test.ts",
  "keywords": [
    "testcafe",
    "testing",
    "automation e2e testing"
  ],
  "scripts": {
    "compile": "node ./node_modules/typescript/lib/tsc --pretty",
    "watch": "node ./node_modules/typescript/lib/tsc -w",
    "clean": "node_modules/.bin/rimraf tmp/ results/",
    "test-live": "node_modules/.bin/testcafe-live chrome tests/test.ts -r spec xunit:results/res.xml -S -s results/screenshots",
    "test-nightmare": "node_modules/.bin/testcafe nightmare tests/test.ts",
    "test": "node_modules/.bin/testcafe chrome tests/test.ts -r spec xunit:results/res.xml -S -s results/screenshots"
  },
  "homepage": "https://github.com/DevExpress/testcafe#readme",
  "devDependencies": {
    "@types/node": "^9.6.6",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.2",
    "testcafe": "^0.19.2",
    "testcafe-angular-selectors": "^0.3.0",
    "testcafe-browser-provider-nightmare": "^0.0.5",
    "testcafe-live": "^0.1.3",
    "tmp": "^0.0.33",
    "ts-node": "^6.0.0",
    "tslib": "^1.9.0",
    "typescript": "^2.8.1"
  }
}
