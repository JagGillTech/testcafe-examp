{
  "name": "testcafe-example",
  "version": "1.0.0",
  "description": "Cloud web application functional end-to-end test project",
  "repository": "https://github.com/hortonworks/cloudbreak-e2e-protractor.git",
  "author": "Hortonworks Inc. (https://hortonworks.com)",
  "license": "MIT",
  "main": "tests/test.ts",
  "keywords": [
    "testcafe",
    "testing",
    "automation e2e testing"
  ],
  "scripts": {
    "compile": "node_modules/.bin/tsc --pretty",
    "watch": "node_modules/.bin/tsc -w",
    "clean": "node_modules/.bin/rimraf tmp/ results/ reports/",
    "test-live": "node_modules/.bin/testcafe-live chrome tests/credentialTest.ts",
    "test-nightmare": "node_modules/.bin/testcafe nightmare tests/*.ts",
    "test": "node_modules/.bin/testcafe chrome tests/*.ts -r spec -S -s results/screenshots --debug-on-fail",
    "test-xml-report": "node_modules/.bin/testcafe chrome tests/*.ts -r xunit:results/report.xml -S -s results/screenshots",
    "test-html-report": "HTML_REPORT_PATH=results node_modules/.bin/testcafe chrome tests/*.ts -r html-testrail"
  },
  "homepage": "https://github.com/DevExpress/testcafe#readme",
  "devDependencies": {
    "@types/node": "^9.6.6",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.2",
    "testcafe": "^0.19.2",
    "testcafe-angular-selectors": "^0.3.0",
    "testcafe-browser-provider-nightmare": "^0.0.5",
    "testcafe-live": "^0.1.3",
    "testcafe-reporter-html-testrail": "^2.0.0",
    "tmp": "^0.0.33",
    "ts-node": "^6.0.0",
    "tslib": "^1.9.0",
    "typescript": "^2.8.1"
  }
}
